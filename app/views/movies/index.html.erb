<div class="banner">
  <h1>Movies</h1>
</div>
<div class="container">
  <div class="container-bar">
    <h1>Movies</h1>
  </div>
  <div class="lists-list">

    <% @movies.each do |movie| %>
      <%= link_to movie_path(movie.id) do %>
        <div class="movie-card">
          <%= movie.title %>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
